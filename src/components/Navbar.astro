---
import { Image } from "@astrojs/image/components";
import hamburgerSVG from "@/assets/icons/hamburger-icon.svg";
import logoSVG from "@/assets/Logo.svg";
---

<header>
  <div class="flex items-center justify-between">
    <div class="h-[48px] w-[42px]">
      <Image src={logoSVG} alt="" width={42} height={48} />
    </div>
    <div class="h-[32px] w-[32px]">
      <Image src={hamburgerSVG} alt="" width={32} height={32} />
    </div>
  </div>
</header>
